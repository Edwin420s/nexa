{
  "name": "nexa-backend",
  "version": "1.0.0",
  "description": "Nexa - Autonomous Research & Build Agent Platform Backend",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:simple": "ts-node-dev --respawn --transpile-only src/dev-server.ts",
    "worker": "ts-node-dev --respawn --transpile-only src/workers/agentWorker.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "npx ts-node src/tests/backend.test.ts",
    "test:quick": "./quick-test.sh",
    "test:full": "./test.sh",
    "test:load": "node load-test.js",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "ai",
    "agents",
    "gemini",
    "research",
    "automation"
  ],
  "author": "Edwin",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "@google/generative-ai": "^0.1.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "winston": "^3.11.0",
    "express-validator": "^7.0.1",
    "uuid": "^9.0.1",
    "bull": "^4.12.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "multer": "^1.4.5-lts.1",
    "socket.io": "^4.6.0",
    "axios": "^1.6.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/compression": "^1.7.5",
    "@types/multer": "^1.4.11",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.3.3",
    "ts-node-dev": "^2.0.0",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.11",
    "ts-jest": "^29.1.1",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}