{
  "agentOutput": {
    "type": "object",
    "properties": {
      "agentName": {
        "type": "string",
        "description": "Name of the agent that generated the output"
      },
      "content": {
        "type": "string",
        "description": "The main output content"
      },
      "confidence": {
        "type": "number",
        "minimum": 0,
        "maximum": 1,
        "description": "Confidence score of the output"
      },
      "metadata": {
        "type": "object",
        "description": "Additional metadata about the output",
        "properties": {
          "model": {
            "type": "string",
            "description": "Model used for generation"
          },
          "tokensUsed": {
            "type": "number",
            "description": "Number of tokens used"
          },
          "executionTime": {
            "type": "number",
            "description": "Execution time in milliseconds"
          },
          "timestamp": {
            "type": "string",
            "format": "date-time",
            "description": "Timestamp of generation"
          }
        }
      }
    },
    "required": ["agentName", "content", "confidence"]
  },
  "researchAgentOutput": {
    "type": "object",
    "properties": {
      "analysis": {
        "type": "string",
        "description": "Comprehensive analysis of the research topic"
      },
      "keyFindings": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "Key findings from the research"
      },
      "sources": {
        "type": "array",
        "items": {
          "type": "object",
          "properties": {
            "title": {
              "type": "string",
              "description": "Title of the source"
            },
            "url": {
              "type": "string",
              "description": "URL of the source (optional)"
            },
            "relevance": {
              "type": "number",
              "minimum": 0,
              "maximum": 1,
              "description": "Relevance score of the source"
            }
          },
          "required": ["title", "relevance"]
        },
        "description": "Sources referenced in the research"
      },
      "recommendations": {
        "type": "array",
        "items": {
          "type": "string"
        },
        "description": "Recommendations based on research"
      }
    },
    "required": ["analysis", "keyFindings", "sources", "recommendations"]
  },
  "codeBuilderOutput": {
    "type": "object",
    "properties": {
      "projectStructure": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "description": "Name of the project"
          },
          "description": {
            "type": "string",
            "description": "Project description"
          },
          "files": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "File name"
                },
                "content": {
                  "type": "string",
                  "description": "File content"
                },
                "language": {
                  "type": "string",
                  "description": "Programming language"
                },
                "type": {
                  "type": "string",
                  "enum": ["component", "utility", "config", "test"],
                  "description": "Type of file"
                }
              },
              "required": ["name", "content", "language", "type"]
            },
            "description": "Generated files"
          },
          "dependencies": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string",
                  "description": "Dependency name"
                },
                "version": {
                  "type": "string",
                  "description": "Dependency version"
                },
                "type": {
                  "type": "string",
                  "enum": ["production", "development"],
                  "description": "Dependency type"
                }
              },
              "required": ["name", "version", "type"]
            },
            "description": "Project dependencies"
          },
          "setupInstructions": {
            "type": "string",
            "description": "Setup and running instructions"
          }
        },
        "required": ["name", "description", "files", "dependencies", "setupInstructions"]
      }
    },
    "required": ["projectStructure"]
  },
  "projectUpdate": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "enum": ["agent_update", "confidence_update", "file_generated", "project_status"],
        "description": "Type of update"
      },
      "projectId": {
        "type": "string",
        "description": "ID of the project"
      },
      "data": {
        "type": "object",
        "description": "Update data"
      },
      "timestamp": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp of the update"
      }
    },
    "required": ["type", "projectId", "data", "timestamp"]
  }
}